[ a                              spdx:LicenseException ;
  rdfs:comment                   "Typically used with GPL-2.0+" ;
  rdfs:seeAlso                   "http://git.denx.de/?p=u-boot.git;a=blob;f=Licenses/Exceptions" ;
  spdx:licenseExceptionId        "u-boot-exception-2.0" ;
  spdx:licenseExceptionTemplate  "<<beginOptional>> The U-Boot License Exception:<<endOptional>>\nEven though U-Boot in general is covered by the GPL-2.0/GPL-2.0+, this does *not* cover the so-called \"standalone\" applications that use U-Boot services by means of the jump table provided by U-Boot exactly for this purpose - this is merely considered normal use of U-Boot, and does *not* fall under the heading of \"derived work\".\nThe header files \"include/image.h\" and \"arch/*/include/asm/u-boot.h\" define interfaces to U-Boot. Including these (unmodified) header files in another file is considered normal use of U-Boot, and does *not* fall under the heading of \"derived work\".\n-- Wolfgang Denk" ;
  spdx:licenseExceptionText      "The U-Boot License Exception:\nEven though U-Boot in general is covered by the GPL-2.0/GPL-2.0+, this does *not* cover the so-called \"standalone\" applications that use U-Boot services by means of the jump table provided by U-Boot exactly for this purpose - this is merely considered normal use of U-Boot, and does *not* fall under the heading of \"derived work\".\nThe header files \"include/image.h\" and \"arch/*/include/asm/u-boot.h\" define interfaces to U-Boot. Including these (unmodified) header files in another file is considered normal use of U-Boot, and does *not* fall under the heading of \"derived work\".\n-- Wolfgang Denk" ;
  spdx:name                      "U-Boot exception 2.0"
] .
